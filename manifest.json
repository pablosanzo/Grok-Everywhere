{
  "manifest_version": 3,
  "name": "Side Panel Toggle",
  "version": "1.0",
  "permissions": [
    "sidePanel"
  ],
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "action": {
    "default_title": "Toggle Side Panel"
  },
  "background": {
    "service_worker": "service-worker.js"
  }
}
